{
  "name": "zds-pickers",
  "version": "2.1.0",
  "description": "Picker controls that are common to ZendrumStudio apps",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && babel src -d dist --ignore **/*.spec.js,*/__config__",
    "build:watch": "babel src -w -d dist",
    "prepublish": "npm run build",
    "lint": "eslint 'src/**'",
    "lint:fix": "npm run lint -- --fix",
    "test": "TZ=America/Los_Angeles NODE_ENV=test jest --env=jsdom",
    "test:watch": "TZ=America/Los_Angeles NODE_ENV=test jest --watchAll",
    "cov": "TZ=America/Los_Angeles NODE_ENV=test jest --coverage",
    "cov:open": "open ./coverage/lcov-report/index.html",
    "cov:watch": "TZ=America/Los_Angeles NODE_ENV=test jest --coverage --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dkadrios/zds-pickers.git"
  },
  "author": "Darin Kadrioski <dkadrios@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dkadrios/zds-pickers/issues"
  },
  "homepage": "https://github.com/dkadrios/zds-pickers#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-syntax-async-generators": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "eslint-watch": "^4.0.2",
    "jest": "^23.6.0",
    "jest-css-modules": "^1.1.0",
    "jest-fetch-mock": "^2.1.0",
    "jest-localstorage-mock": "^2.4.0"
  },
  "dependencies": {
    "@material-ui/core": "^3.8.1",
    "classnames": "^2.2.6",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-select": "^2.2.0"
  }
}
