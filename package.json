{
  "name": "zds-pickers",
  "version": "1.5.0",
  "description": "Picker controls that are common to ZendrumStudio apps",
  "main": "build/index.js",
  "scripts": {
    "build": "babel lib -d build --ignore __config__,**/*.spec.js",
    "build:watch": "babel lib -w -d build",
    "prepublish": "npm run build",
    "lint": "eslint 'lib/**'",
    "lint:fix": "npm run lint -- --fix",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watchAll",
    "test:cov": "NODE_ENV=test jest --coverage",
    "test:cov:open": "open ./coverage/lcov-report/index.html",
    "test:cov:watch": "NODE_ENV=test jest --coverage --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dkadrios/zds-pickers.git"
  },
  "author": "Darin Kadrioski <dkadrios@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dkadrios/zds-pickers/issues"
  },
  "homepage": "https://github.com/dkadrios/zds-pickers#readme",
  "engines": {
    "node": "^8.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-watch": "^4.0.2",
    "jest": "^23.4.1",
    "jest-css-modules": "^1.1.0",
    "jest-fetch-mock": "^1.6.5",
    "jest-localstorage-mock": "^2.2.0",
    "polished": "^2.1.1"
  },
  "dependencies": {
    "@material-ui/core": "^3.1.0",
    "autosuggest-highlight": "^3.1.1",
    "classnames": "^2.2.6",
    "lodash": "^4.17.11",
    "npm-check-updates": "^2.14.2",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-autosuggest": "^9.4.2",
    "react-dom": "^16.5.2"
  }
}
