{
  "name": "zds-pickers",
  "version": "3.1.0",
  "homepage": "https://github.com/dkadrios/zds-pickers#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist/**/*.js",
    "README.md",
    "LICENSE"
