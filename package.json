{
  "name": "zds-pickers",
  "version": "1.5.0",
  "description": "Picker controls that are common to ZendrumStudio apps",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src -d dist --ignore __config__,**/*.spec.js",
    "build:watch": "babel src -w -d dist",
    "prepublish": "npm run build",
    "lint": "eslint 'src/**'",
    "lint:fix": "npm run lint -- --fix",
    "test": "TZ=America/Los_Angeles NODE_ENV=test jest --env=jsdom",
    "test:watch": "TZ=America/Los_Angeles NODE_ENV=test jest --watchAll",
    "cov": "TZ=America/Los_Angeles NODE_ENV=test jest --coverage",
    "cov:open": "open ./coverage/lcov-report/index.html",
    "cov:watch": "TZ=America/Los_Angeles NODE_ENV=test jest --coverage --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dkadrios/zds-pickers.git"
  },
  "author": "Darin Kadrioski <dkadrios@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dkadrios/zds-pickers/issues"
  },
  "homepage": "https://github.com/dkadrios/zds-pickers#readme",
  "engines": {
    "node": "^8.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "eslint-watch": "^4.0.2",
    "jest": "^23.6.0",
    "jest-css-modules": "^1.1.0",
    "jest-fetch-mock": "^1.6.6",
    "jest-localstorage-mock": "^2.2.0",
    "polished": "^2.2.0"
  },
  "dependencies": {
    "@material-ui/core": "^3.1.2",
    "autosuggest-highlight": "^3.1.1",
    "classnames": "^2.2.6",
    "lodash": "^4.17.11",
    "npm-check-updates": "^2.14.2",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-autosuggest": "^9.4.2",
    "react-dom": "^16.5.2"
  }
}
