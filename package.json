  "author": "Darin Kadrioski <dkadrios@gmail.com>",
  "bugs": {
    "url": "https://github.com/dkadrios/zds-pickers/issues"
  },
  "description": "Picker controls that are common to Zendrum Studio apps",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dkadrios/zds-pickers.git"
  },
  "scripts": {
    "build": "npm run clean && babel src -d dist --ignore **/*.spec.js,*/__config__",
    "build:watch": "babel src -w -d dist",
    "clean": "rimraf dist",
    "cov": "TZ=America/Los_Angeles NODE_ENV=test jest --coverage",
    "cov:open": "open ./coverage/lcov-report/index.html",
    "cov:watch": "TZ=America/Los_Angeles NODE_ENV=test jest --coverage --watchAll",
    "lint": "eslint 'src/**'",
    "lint:fix": "npm run lint -- --fix",
    "prepublish": "npm run build",
    "test": "TZ=America/Los_Angeles NODE_ENV=test jest --env=jsdom",
    "test:watch": "TZ=America/Los_Angeles NODE_ENV=test jest --watchAll"
  },
  "devDependencies": {
